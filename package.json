{
  "name": "mf-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:shell",
    "start:shell": "ng serve shell  -o --port 5000",
    "start:juridic": "ng serve juridic -o --port 4202",
    "start:authentication": "ng serve authentication -o --port 3002",
    "start:all": "concurrently \"npm run start:shell\" \"npm run start:juridic\"  \"npm run start:authentication\" ",
    "build:shell": "ng build shell",
    "build:juridic": "ng build juridic",
    "build:authentication": "ng build authentication",
    "prebuild": "shx cp -r projects/shared-lib/src/lib/i18n/*.json projects/shell/src/assets/i18n/",
    "serve:dist": "concurrently \"serve dist/shell -l 5000 -s\"  \"serve dist/juridic -l 4202 -s\"  \"serve dist/autentication -l 3002 -s\" ",
    "build": "npm run build:shell && npm run build:juridic && npm run build:authentication",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^16.0.0",
